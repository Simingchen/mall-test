<van-tree-select
  items="{{ items }}"
  main-active-index="{{ mainActiveIndex }}"
  active-id="{{ activeId }}"
  bind:click-nav="onClickNav"
  bind:click-item="onClickItem"
  height="100vh"
>
  <view slot="content">
    <van-empty wx:if="{{!childrenList.length}}" description="暂无相关信息~" />
  </view>
</van-tree-select>
<!-- tabbar -->
<tabBar selected="1" cartNum="{{cartNum}}"/>