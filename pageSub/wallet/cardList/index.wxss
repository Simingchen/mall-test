page{
  background: #f8f8f8;
  min-height: 100vh;
}
.van-address-list {
  box-sizing: border-box;
  height: 100%;
  padding: 12px 12px 80px;
}
.bottom-btn{
  position: fixed;
  bottom:0;
  padding: 20rpx;
  width: 100%;
  background: #fff;
}
.van-address-item {
  padding: 12px;
  margin-bottom: 20rpx;
  border-radius: 8px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  width: 100%;
  overflow: hidden;
  color: #323233;
  font-size: 14px;
  line-height: 24px;
  background-color: #fff;
}

.van-cell__value {
  position: relative;
  overflow: hidden;
  color: #323233;
  text-align: left;
  vertical-align: middle;
  word-wrap: break-word;
}
.van-cell__title, .van-cell__value {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.van-address-item__value {
  padding-right: 44px;
}
.van-address-item__name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  margin-bottom: 8px;
  font-size: 16px;
  line-height: 22px;
}
.van-address-item__address {
  color: #323233;
  font-size: 13px;
  line-height: 18px;
}
.van-tag {
  position: relative;
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: inline-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 0 4px;
  color: #fff;
  background-color: #ee0a24;
  font-size: 12px;
  line-height: 16px;
  border-radius: 16px;
  -webkit-box-flex: 0;
  -webkit-flex: none;
  flex: none;
  margin-left: 8px;
  padding-top: 0;
  padding-bottom: 0;
  line-height: 1.4em;
}
