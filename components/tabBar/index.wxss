.tabbar-box{
  position: fixed;
  bottom:0;
  left:0;
  width: 100%;
  height: 98rpx;
  z-index: 100;
  display: flex;
  text-align: center;
  color:#d7d7d7;
  background: #fff;
  border-top: 1rpx solid #f2f2f2;
}
.tabbar-box .item{
  position: relative;
  flex: 1;
  font-size: 20rpx;
  color:#333333FF;
  line-height: 30rpx;
}
.cartNum {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
    top: 0;
    right: 0;
    min-width: 16px;
    padding: 0 3px;
    color: #fff;
    font-weight: 500;
    font-size: 12px;
    line-height: 1.2;
    text-align: center;
    background-color: #ee0a24;
    border: 1px solid #fff;
    border-radius: 16px;
    transform: translate(-100%, 0);
    transform-origin: 100%;
}
.tabbar-box .iconfont{
  margin-top: 10rpx;
  height: 52rpx;
  font-size: 52rpx;
  line-height: 52rpx;
  color:#999;
}
.hr100{
  height: 120rpx;
}
.tabbar-box .active{
  color:#00a5a5;
}
.tabbar-box .active .iconfont{
  color:#00a5a5;
}